{
  "name": "nauka-gita",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css/*",
    "build:sass": "node-sass --output-style -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "watch": "npm-run-all build watch:*",
    "watch:sassprefixer": "onchange sass/*.scss -- npm-run-all build:sass build:autoprefixer",
    "watch:browsersync": "browser-sync --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiroslawChmielecki/nauka-gita.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MiroslawChmielecki/nauka-gita/issues"
  },
  "homepage": "https://github.com/MiroslawChmielecki/nauka-gita#readme",
  "dependencies": {
    "browser-sync": "^2.26.3"
  },
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "globstar": "^1.0.0",
    "html-validator-cli": "^5.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3"
  }
}
